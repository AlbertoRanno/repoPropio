<div>
  <ul>
    <% for (let i = 1; i <= 10; i++) { %> <% if ( i % 2 == 0) { %>
    <li class="par">Soy el item numero <%= i %></li>
    <% } else { %>
    <li class="impar">Soy el item numero <%= i %></li>
    <% } %> <% } %>
  </ul>
</div>

.par { border: #4caf50 4 px solid; font-size: xx-large; } 
.impar { border:rebeccapurple 4 px solid; color: #009688; }


